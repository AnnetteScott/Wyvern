{
  "name": "wyvern",
  "version": "2.0.0",
  "description": "A simple Wyvern App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-installer": "electron-builder"
  },
"build": {
	"appId": "Wyvern-app",
	"win": {
		"target": "nsis",
    "requestedExecutionLevel": "requireAdministrator"

  },
	"icon": "Images/WyvernIcon.ico",

		"nsis": {
		"installerIcon": "Images/WyvernIcon.ico",
		"uninstallerIcon": "Images/WyvernIcon.ico",
		"uninstallDisplayName": "Wyvern",
		"oneClick": false,
		"allowToChangeInstallationDirectory": true
	},
	"extraFiles": [
		"./DATA"
	]
},

  "repository": {
    "type": "git",
    "url": "git+https://github.com/NotNatural21/Wyvern.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NotNatural21/Wyvern/issues"
  },
  "homepage": "https://github.com/NotNatural21/Wyvern#readme",
  "devDependencies": {
    "electron": "16.0.7",
    "electron-builder": "^22.14.5"
  },
  "dependencies": {
    "electron-reload": "^2.0.0-alpha.1"
  }
}
